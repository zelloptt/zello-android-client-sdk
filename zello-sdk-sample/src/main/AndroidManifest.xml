<?xml version="1.0" encoding="utf-8"?>
<manifest
	xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.zello.sdk.sample">

	<application
		android:name="com.zello.sdk.sample.App"
		android:allowBackup="false"
		android:fullBackupContent="false"
		android:icon="@mipmap/ic_launcher"
		android:label="@string/app_name"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">

		<activity
			android:name="com.zello.sdk.sample.TalkActivity"
			android:label="@string/app_name"
			android:exported="true">

			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
				<category android:name="android.intent.category.MULTIWINDOW_LAUNCHER" />
			</intent-filter>
		</activity>

		<activity
			android:name="com.zello.sdk.sample.AnotherActivity"
			android:label="@string/app_name" />

		<meta-data
			android:name="com.sec.android.support.multiwindow"
			android:value="true" />

		<meta-data
			android:name="com.samsung.android.sdk.multiwindow.penwindow.enable"
			android:value="true" />

		<meta-data
			android:name="com.samsung.android.sdk.multiwindow.style"
			android:value="fixedRatio" />
	</application>
</manifest>
